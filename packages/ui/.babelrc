{
  "presets": [
    "@babel/typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 1 versions", "ie >= 11", "Firefox ESR"]
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-proposal-class-properties"
  ]
}
